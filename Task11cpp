11. 1000-dən kiçik Fibonacci ədədlərini çap edin.
#include <iostream>
using namespace std;

int main() {
    int a = 0, b = 1, c;
    cout << "Fibonacci ədədləri (1000-dən kiçik): " << a << " " << b << " ";
    while (true) {
        c = a + b;
        if (c >= 1000) break;
        cout << c << " ";
        a = b;
        b = c;
    }
    return 0;
}
