7. Armstrong ədədləri tapın.
#include <iostream>
#include <cmath>
using namespace std;

int main() {
    cout << "Armstrong ədədlər (100–999): ";
    for (int i = 100; i <= 999; i++) {
        int a = i / 100, b = (i / 10) % 10, c = i % 10;
        if (pow(a, 3) + pow(b, 3) + pow(c, 3) == i)
            cout << i << " ";
    }
    return 0;
}
